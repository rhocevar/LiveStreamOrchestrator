{
  "name": "favorited",
  "version": "1.0.0",
  "description": "Favorited - Livestream Orchestrator with React Client and Node.js API",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "npm run dev:server & npm run dev:client",
    "dev:server": "npm run dev --prefix server",
    "dev:server:debug": "npm run dev:debug --prefix server",
    "dev:client": "npm run dev --prefix client",
    "build": "npm run build:server && npm run build:client",
    "build:server": "npm run build --prefix server",
    "build:client": "npm run build --prefix client",
    "install:all": "npm install --prefix client && npm install --prefix server",
    "prisma:generate": "npm run prisma:generate --prefix server",
    "prisma:migrate": "npm run prisma:migrate --prefix server",
    "prisma:studio": "npm run prisma:studio --prefix server"
  },
  "keywords": [
    "livestream",
    "livekit",
    "react",
    "typescript",
    "node",
    "monorepo"
  ],
  "author": "",
  "license": "ISC"
}
